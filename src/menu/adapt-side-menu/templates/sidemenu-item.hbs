<div class="menu-item-inner" aria-label="{{_globals._menu._sidemenu.menuItem}}" {{#if _globals._menu._sidemenu.menuItem}}tabindex="0"{{/if}}>
    <div class="menu-item-innerest">
    <div class="menu-item-graphic">
        {{#if _graphic.src}}
            <img src="{{_graphic.src}}" alt="{{_graphic.alt}}" />
        {{/if}}
    </div>
    <div class="menu-item-title">
        <div class="menu-item-title-inner h3 accessible-text-block" role="heading" aria-level="2" tabindex="0">{{{displayTitle}}}</div>
    </div>
    <div class="menu-item-body">
        <div class="menu-item-body-inner">{{{a11y_text body}}}</div>
    </div>
        </div>
    <div class="menu-item-button">
        <button aria-label="{{linkText}} {{#if _isVisited}}{{_globals._accessibility._ariaLabels.visited}}{{/if}}" class="{{#if _isVisited}}visited{{/if}} {{#if _isLocked}}disabled{{/if}} accessible-text-block" {{#if _isLocked}}disabled="disabled"{{/if}}>
            {{#if _isLocked}}

            Locked

            {{else if _isOptional}}

            Optional

            {{else if _isComplete}}

            Complete

            {{else if _isVisited}}

            In Progress




            {{else}}


            {{{linkText}}}

            {{/if}}
        </button>
           {{#if duration}}
            <span class="menu-item-duration accessible-text-block" role="region" tabindex="0">{{{_globals._menu._sidemenu.durationLabel}}} {{{duration}}}</span>
        {{/if}}
       
    </div>
</div>

